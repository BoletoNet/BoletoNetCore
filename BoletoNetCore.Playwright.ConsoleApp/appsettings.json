{
  "PlaywrightPdf": {
    "Renderer": {
      "MaxConcurrency": 2,
      "RenderTimeoutSeconds": 30,
      "PrewarmOnStart": true,
      "RestartOnFailure": true,
      "BrowserExecutablePath": null,
      "BrowserLaunchArgs": null
    },
    "Options": {
      "Format": "A4",
      "Landscape": false,
      "PrintBackground": true,
      "PreferCssPageSize": false,
      "Scale": 1.0,
      "Margin": {
        "Top": "10mm",
        "Bottom": "10mm",
        "Left": "10mm",
        "Right": "10mm"
      }
    }
  }
}
